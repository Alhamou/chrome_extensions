{
    "manifest_version" : 2,
    "name" : "Alhamou",
    "version" : "0.1",
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "background" : {
        "scripts": ["scripts/eventPage.js", "scripts/options.js"],
        "persistent" : false
    },

    "options_page": "options.html",



    "permissions": [
        "storage",
        "notifications",
        "contextMenus"
    ],
    "icons": {
        "16": "icon.png"
      }
}
